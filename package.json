{
	"name": "v1",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"check-types": "turbo run check-types",
		"build:packages": "turbo run build --filter='./packages/*'",
		"docker:pull": "docker compose --file ./docker-compose.yml --file ./docker-compose.ci.yml pull",
		"docker:up": "docker-compose --file ./docker-compose.yml --project-name dms-pro up",
		"docker:build": "docker-compose --file ./docker-compose.yml build"
	},
	"devDependencies": {
		"prettier": "^3.7.4",
		"turbo": "^2.7.6",
		"typescript": "5.9.2"
	},
	"packageManager": "pnpm@9.0.0",
	"engines": {
		"node": ">=18"
	}
}
