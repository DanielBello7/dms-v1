{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@app/constants": [
        "libs/constants/src"
      ],
      "@app/constants/*": [
        "libs/constants/src/*"
      ],
      "@app/logger": [
        "libs/logs/src"
      ],
      "@app/logs/*": [
        "libs/logs/src/*"
      ],
      "@app/mutations": [
        "libs/mutations/src"
      ],
      "@app/mutations/*": [
        "libs/mutations/src/*"
      ],
      "@app/security": [
        "libs/security/src"
      ],
      "@app/security/*": [
        "libs/security/src/*"
      ],
      "@app/util": [
        "libs/util/src"
      ],
      "@app/util/*": [
        "libs/util/src/*"
      ],
      "@app/winston": [
        "libs/winston/src"
      ],
      "@app/winston/*": [
        "libs/winston/src/*"
      ]
    }
  }
}